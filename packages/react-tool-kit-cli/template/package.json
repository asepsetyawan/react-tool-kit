{
  "name": "~~name~~",
  "version": "1.0.6",
  "description": "react ssr starter",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "bnr start",
    "clean": "@asep.setiawan/react-tool-kit clean",
    "lint": "@asep.setiawan/react-tool-kit lint",
    "dll": "bnr dll",
    "test": "jest",
    "testc": "npm t -- --coverage",
    "build": "npm run deploy:production",
    "deploy:dev": "bnr deploy:dev",
    "deploy:staging": "bnr deploy:staging",
    "deploy:production": "bnr deploy:production",
    "serve": "bnr serve",
    "serve:lerna": "bnr serve:lerna",
    "prettier": "prettier --write \"{src,config}/**/*.js\""
  },
  "betterScripts": {
    "start": {
      "command": "@asep.setiawan/react-tool-kit start",
      "env": {
        "NODE_ENV": "development",
        "APP_ENV": "development",
        "APP_ASSET_PATH": "/myapp/"
      }
    },
    "dll": {
      "command": "@asep.setiawan/react-tool-kit build-dll",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "serve": {
      "command": "node dist/bundle.js",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "production"
      }
    },
    "serve:lerna": {
      "command": "node --preserve-symlinks dist/bundle.js",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "production"
      }
    },
    "deploy:dev": {
      "command": "@asep.setiawan/react-tool-kit build",
      "env": {
        "NODE_ENV": "development",
        "APP_ENV": "development",
        "APP_ASSET_PATH": "/myapp/"
      }
    },
    "deploy:staging": {
      "command": "@asep.setiawan/react-tool-kit build",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "staging",
        "APP_ASSET_PATH": "/myapp/"
      }
    },
    "deploy:production": {
      "command": "@asep.setiawan/react-tool-kit build",
      "env": {
        "NODE_ENV": "production",
        "APP_ENV": "production",
        "APP_ASSET_PATH": "/myapp/"
      }
    }
  },
  "author": "User",
  "browserslist": [
    "> 1% in ID",
    "last 2 versions",
    "Firefox ESR",
    "iOS >= 9",
    "not ie < 11"
  ],
  "jest": {
    "testURL": "http://localhost",
    "collectCoverageFrom": [
      "src/**/*.{js}"
    ],
    "moduleNameMapper": {
      "^lodash-es$": "lodash"
    },
    "testRegex": "(src)/.*\\.spec\\.js$",
    "globals": {
      "__DEV__": false,
      "__TEST__": true,
      "__PROD__": false,
      "__STAG__": false
    }
  },
  "dependencies": {
    "@asep.setiawan/react-tool-kit-server": "^1.0.6",
    "@babel/polyfill": "^7.0.0",
    "@loadable/component": "5.6.0",
    "apollo-boost": "0.1.12",
    "axios": "0.18.0",
    "basscss": "^8.0.4",
    "better-npm-run": "^0.1.1",
    "classnames": "^2.2.6",
    "dotenv": "6.0.0",
    "graphql": "0.13.2",
    "graphql-tag": "2.9.2",
    "lodash": "4.17.10",
    "lodash-es": "4.17.10",
    "npm-run-all": "4.1.3",
    "prop-types": "^15.6.2",
    "query-string": "^6.1.0",
    "raf": "3.4.0",
    "react": "16.8.6",
    "react-apollo": "2.5.6",
    "react-body-classname": "^1.2.0",
    "react-dom": "16.8.6",
    "react-helmet-async": "0.1.0",
    "react-hot-loader": "4.11.1",
    "react-redux": "5.0.7",
    "react-router-config": "5.0.1",
    "react-router-dom": "5.0.1",
    "recompose": "0.30.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "ua-parser-js": "0.7.18"
  },
  "devDependencies": {
    "@asep.setiawan/react-tool-kit": "^1.0.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "23.6.0",
    "http-proxy-middleware": "^0.18.0",
    "jest": "^23.6.0",
    "prettier": "1.14.0"
  }
}
